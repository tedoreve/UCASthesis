@Article{Kostunin2015,
  Title                    = {Reconstruction of air-shower parameters for large-scale radio detectors using the lateral distribution},
  Author                   = {D. Kostunin and P. A. Bezyazeekov and R. Hiller and F. G. Schröder and V. Lenok and E. Levinson},
  Journal                  = {Astropart. Phys.},
  Year                     = {2015},

  Month                    = apr,
  Pages                    = {79},
  Volume                   = {74},

  Abstract                 = {We investigate features of the lateral distribution function (LDF) of the radio signal emitted by cosmic ray air-showers with primary energies $> 0.1$~EeV and its connection to air-shower parameters such as energy and shower maximum using CoREAS simulations made for the configuration of the Tunka-Rex antenna array. Taking into account all significant contributions to the total radio emission, such as by the geomagnetic effect, the charge excess, and the atmospheric refraction we parameterize the radio LDF. This parameterization is two-dimensional and has several free parameters. The large number of free parameters is not suitable for experiments of sparse arrays operating at low SNR (signal-to-noise ratios). Thus, exploiting symmetries, we decrease the number of free parameters and reduce the LDF to a simple one-dimensional function. The remaining parameters can be fit with a small number of points, i.e. as few as the signal from three antennas above detection threshold. Finally, we present a method for the reconstruction of air-shower parameters, in particular, energy and $X_{\mathrm{max}}$ (shower maximum), which can be reached with a theoretical accuracy of better than 15\% and 30~g/cm$^2$, respectively.},
  Comments                 = {11 pages, 8 figures, 1 table},
  Eprint                   = {1504.05083},
  Oai2identifier           = {1504.05083},
  Owner                    = {tedoreve},
  Timestamp                = {2016.09.23}
}

@Article{Koyama1995,
  Title                    = {{Evidence for shock acceleration of high-energy electrons in the supernova remnant SN1006}},
  Author                   = {{Koyama}, K. and {Petre}, R. and {Gotthelf}, E.~V. and {Hwang}, U. and {Matsuura}, M. and {Ozaki}, M. and {Holt}, S.~S.},
  Journal                  = {\nat},
  Year                     = {1995},

  Month                    = nov,
  Pages                    = {255-258},
  Volume                   = {378},
  Abstract                 = {HIGH-ENERGY cosmic rays (relativistic heavy nuclei) play an important role in heating interstellar matter in the Milky Way1,2, and they affect chemical abundances through collisions with atoms in the interstellar gas2. Although it has long been thought that these cosmic rays arise from supernovae3,4, direct evidence for such an association has been lacking. Here we report X-ray observations of the remnant of supernova 1006, made by the ASCA satellite, which indicate that emission from the edges of the remnant shell is dominated by radiation from electrons accelerated to energies of ˜ 100 TeV within the shock front. Ions in the shell are likely to have been accelerated to similar energies, thus giving rise to very-high-energy cosmic rays.},
  Adsnote                  = {Provided by the SAO/NASA Astrophysics Data System},
  Adsurl                   = {http://adsabs.harvard.edu/abs/1995Natur.378..255K},
  Doi                      = {10.1038/378255a0}
}

@Article{Kozlova2016,
  Title                    = {{The first observation of an intermediate flare from SGR 1935+2154}},
  Author                   = {{Kozlova}, A.~V. and {Israel}, G.~L. and {Svinkin}, D.~S. and {Frederiks}, D.~D. and {Pal'shin}, V.~D. and {Tsvetkova}, A.~E. and {Hurley}, K. and {Goldsten}, J. and {Golovin}, D.~V. and {Mitrofanov}, I.~G. and {Zhang}, X.-L.},
  Journal                  = {\mnras},
  Year                     = {2016},

  Month                    = aug,
  Pages                    = {2008-2014},
  Volume                   = {460},

  Abstract                 = {We report on the bright burst detected by four Interplanetary network (IPN) spacecraft on 2015 April 12. The IPN localization of the source is consistent with the position of the recently discovered soft gamma-repeater SGR 1935+2154. From the Konus-Wind (KW) observation, we derive temporal and spectral parameters of the emission, and the burst energetics. The rather long duration of the burst (˜1.7 s) and the large measured energy fluence (˜2.5 × 10-5 erg cm-2) put it in the class of rare `intermediate' soft gamma-repeater (SGR) flares, and this is the first one observed from SGR 1935+2154. A search for quasi-periodic oscillations in the KW light curve yields no statistically significant signal. Of four spectral models tested, optically thin thermal bremsstrahlung and a single blackbody (BB) function can be rejected on statistical grounds; two more complex models, a cutoff power law (CPL) and a sum of two BB functions (2BB), fit the burst spectra well and neither of them may be ruled out by the KW observation. The CPL and 2BB model parameters we report for this bright flare are typical of SGRs; they are also consistent with those obtained from observations of much weaker and shorter SGR 1935+2154 bursts with other instruments. From the distribution of 2BB spectral fit parameters we estimate the SGR 1935+2154 distance to be <10.0 kpc, in agreement with that of the Galactic supernova remnant G57.2+0.8 at 9.1 kpc.},
  Adsnote                  = {Provided by the SAO/NASA Astrophysics Data System},
  Adsurl                   = {http://adsabs.harvard.edu/abs/2016MNRAS.460.2008K},
  Archiveprefix            = {arXiv},
  Doi                      = {10.1093/mnras/stw1109},
  Eprint                   = {1605.02993},
  Keywords                 = {stars: magnetars, pulsars: individual: SGR 1935+2154, gamma-rays: stars},
  Owner                    = {tedoreve},
  Primaryclass             = {astro-ph.HE},
  Timestamp                = {2016.09.23}
}

