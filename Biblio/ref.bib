  Keywords                 = {ISM: Individual: Name: Crab Nebula, Stars: Pulsars: Individual: Name: Crab Pulsar, Radiation Mechanisms: Nonthermal, Stars: Neutron, ISM: Supernova Remnants, X-Rays: General},
  Owner                    = {tedoreve},
