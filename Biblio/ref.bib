  Eprint                   = {1009.0894},
  Keywords                 = {radiation mechanisms: non-thermal, ISM: individual objects: W28 (G6.4-0.1), ISM: supernova remnants, gamma-rays: ISM},
  Primaryclass             = {astro-ph.HE}
}

