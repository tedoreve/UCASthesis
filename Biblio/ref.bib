@Article{Shull1991,
  Title                    = {{A kinematic and proper-motion survey of the Cygnus Loop}},
  Author                   = {{Shull}, Jr., P. and {Hippelein}, H.},
  Journal                  = {\apj},
  Year                     = {1991},

  Month                    = dec,
  Pages                    = {714-728},
  Volume                   = {383},

  Abstract                 = {H-alpha radial velocities, line widths, and line intensities based on 71 Fabry-Perot interferometer scans of 61 fields in the Cygnus Loop supernova remnant SNR are reported. Also, measurements of proper motions at 39 locations within the Cygnus Loop are summarized. These results show that the remnant is expanding asymmetrically into the ambient interstellar medium, with rest-frame velocities as high as 380 km/s in the near half and 150 km/s in the far half. This can plausibly be interpreted as the result of the progenitor's evolution near a density discontinuity, the consequent formation of a cavity and partial shell, and the SN's interaction with that shell. The remnant's optically observed filamentary structure results from line-of-sight effects as shock waves advance into the irregular inner surface of the shell. In this context, the derived expansion velocities and proper motions give a formal distance to the Cygnus Loop of roughly 600 pc, with 1-sigma limits of 300 and 1200 pc.},
  Adsnote                  = {Provided by the SAO/NASA Astrophysics Data System},
  Adsurl                   = {http://adsabs.harvard.edu/abs/1991ApJ...383..714S},
  Doi                      = {10.1086/170828},
  Keywords                 = {Astronomical Spectroscopy, Emission Spectra, H Alpha Line, Stellar Evolution, Stellar Motions, Supernova Remnants, Data Reduction, Interferometry, Radial Velocity, Stellar Physics},
  Owner                    = {tedoreve},
  Timestamp                = {2016.09.23}
}

