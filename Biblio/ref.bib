  Author                   = {{Enomoto}, R. and {Higashi}, Y. and {Yoshida}, T. and {Tanimori}, T. and {Bicknell}, G.~V. and {Clay}, R.~W. and {Edwards}, P.~G. and {Gunji}, S. and {Hara}, S. and {Hara}, T. and {Hattori}, T. and {Hayashi}, S. and {Hirai}, Y. and {Inoue}, K. and {Kabuki}, S. and {Kajino}, F. and {Katagiri}, H. and {Kawachi}, A. and {Kifune}, T. and {Kiuchi}, R. and {Kubo}, H. and {Kushida}, J. and {Matsubara}, Y. and {Mizukami}, T. and {Mizumoto}, Y. and {Mizuniwa}, R. and {Mori}, M. and {Muraishi}, H. and {Muraki}, Y. and {Naito}, T. and {Nakamori}, T. and {Nakano}, S. and {Nishida}, D. and {Nishijima}, K. and {Ohishi}, M. and {Sakamoto}, Y. and {Seki}, A. and {Stamatescu}, V. and {Suzuki}, T. and {Swaby}, D.~L. and {Thornton}, G. and {Tokanai}, F. and {Tsuchiya}, K. and {Watanabe}, S. and {Yamada}, Y. and {Yamazaki}, E. and {Yanagita}, S. and {Yoshikoshi}, T. and {Yukawa}, Y.},
  Journal                  = {\apj},
