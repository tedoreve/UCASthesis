  eprint        = {astro-ph/0512086},
  keywords      = {(ISM:) cosmic rays, Acceleration of particles, Shock waves, Supernovae individual (Tycho's SNR), Radiation mechanisms: non-thermal, Gamma-rays: theory},
}
