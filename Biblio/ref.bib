}

@Article{Chevalier1982,
  Title                    = {{Self-similar solutions for the interaction of stellar ejecta with an external medium}},
  Author                   = {{Chevalier}, R.~A.},
  Journal                  = {\apj},
  Year                     = {1982},

  Month                    = jul,
  Pages                    = {790-797},
  Volume                   = {258},

  Abstract                 = {The interaction of a uniformly expanding gas with a stationary ambient medium is studied in the case where the expanding matter has a power-law density profile (ρ &propto; r-n), as well as the ambient medium (ρ &propto; r-s). Self-similar solutions for the structure of the interaction region can be found provided that s<3 and n>5. Models for Type I supernovae indicate that a ρ &propto; r-7 profile may occur in the outer part of the expanding star. The n = 7, s = 0 solution is applied to the remnant of Tycho's supernova, and it is found to reproduce the basic dynamical and morphological properties of the remnant. The model may also explain the lack of strong Fe line emission in the X-ray spectrum. The solutions with s = 2 are applicable to Type II supernovae because the expanding supernova envelopes probably interact with regions of presupernova mass loss. The properties of the interactions region are important for the X-ray emission from Type II supernovae.},
  Adsnote                  = {Provided by the SAO/NASA Astrophysics Data System},
  Adsurl                   = {http://adsabs.harvard.edu/abs/1982ApJ...258..790C},
  Doi                      = {10.1086/160126},
  Keywords                 = {Interstellar Matter, Stellar Mass Ejection, Supernova Remnants, Density Distribution, Gas Density, Gas-Gas Interactions, Gas-Solid Interactions, Light Curve, X Ray Spectra}
}

@Article{Chevalier1989,
  Title                    = {{The interaction of supernovae with circumstellar bubbles}},
  Author                   = {{Chevalier}, R.~A. and {Liang}, E.~P.},
  Journal                  = {\apj},
  Year                     = {1989},

  Month                    = sep,
  Pages                    = {332-340},
  Volume                   = {344},

  Abstract                 = {This paper examines the interaction of a massive star supernova with the shell created by the fast wind from a blue supergiant, either in the main-sequence phase or in a late evolutionary phase. Making a number of idealizations, the general features of shell interaction are described by semianalytical solutions. The expected properties of the supernova and its environment are discussed, and the hydrodynamics of the interaction is described. It is found that, typically, the shock traversal occurs before the energy transfer is significant. Applications of the model to observed objects are considered, with special attention given to the interaction of SN 1987A with its circumstellar shell, which is expected to occur within decades.},
  Adsnote                  = {Provided by the SAO/NASA Astrophysics Data System},
  Adsurl                   = {http://adsabs.harvard.edu/abs/1989ApJ...344..332C},
  Doi                      = {10.1086/167802},
  Keywords                 = {Stellar Envelopes, Stellar Winds, Supernova Remnants, Supernovae, Radio Sources (Astronomy), Shock Waves, Stellar Evolution, Stellar Mass, Supernova 1987a, X Ray Sources}
}

@Article{Chugai2014,
  Title                    = {{Does the energy of type IIP supernovae depend on the stellar mass?}},
  Author                   = {{Chugai}, N.~N. and {Utrobin}, V.~P.},
  Journal                  = {Astronomy Letters},
  Year                     = {2014},

  Month                    = may,
  Pages                    = {291-300},
  Volume                   = {40},

  Abstract                 = {The oxygen density in the central zone of the ejecta of nine type IIP supernovae (SNe IIP) at the nebular phase has been determined from the [O I] 6300, 6364 Å doublet lines. In combination with the known estimates for two supernovae, the results of measurements show that the oxygen densities on day 300 are distributed in a narrow range, (2.3 ± 1) × 109 cm-3. This result does not depend on the distance, extinction, and model assumptions. Analysis of the density distribution found leads to the conclusion that the SN IIP explosion energy increases with stellar mass.},
