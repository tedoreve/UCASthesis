@Article{vCadevz2004,
  Title                    = {{Spectroscopy and Three-Dimensional Imaging of the Crab Nebula}},
  Author                   = {{{\v C}ade{\v z}}, A. and {Carrami{\~n}ana}, A. and {Vidrih}, S. },
  Journal                  = {\apj},
  Year                     = {2004},

  Month                    = jul,
  Pages                    = {797-809},
  Volume                   = {609},

  Abstract                 = {Spectroscopy of the Crab Nebula along different slit directions reveals the three-dimensional structure of the optical nebula. On the basis of the linear radial expansion result first discovered by V. Trimble in 1968, we make a three-dimensional model of the optical emission. Results from a limited number of slit directions suggest that optical lines originate from a complicated array of wisps that are located in a rather thin shell pierced by a jet. The jet is certainly not prominent in optical emission lines, but the direction of the piercing is consistent with the direction of the X-ray and radio jet. The shell's effective radius is ~79", its thickness is about a third of the radius, and it is moving out with an average velocity of 1160 km s- 1.},
  Adsnote                  = {Provided by the SAO/NASA Astrophysics Data System},
  Adsurl                   = {http://adsabs.harvard.edu/abs/2004ApJ...609..797C},
  Doi                      = {10.1086/421234},
  Eprint                   = {astro-ph/0403153},
  Keywords                 = {ISM: Individual: Name: Crab Nebula, ISM: Kinematics and Dynamics, ISM: Supernova Remnants, Techniques: Spectroscopic},
  Owner                    = {tedoreve},
  Timestamp                = {2016.09.23}
}

